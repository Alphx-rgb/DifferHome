  <div class="container main-container">
    <div id="map" class="map">
      <!-- <div class="circle checkbox">  <input class="map-button" type="checkbox" id="toggle" name="toggle">
      <label for="toggle"></label>
      </div> -->
      <!-- <div class="expand-img "><img src="./expand.png" alt=""></div> -->
      <!-- <div class="shrink-img hide">
        <img src="./shrink.png" alt="">
      </div> -->
    </div>

    <div class="images">
      <img src="./images/<%=product.images[0]%>" class="main-image" alt="">
      <div class="side-images">
        <img src="./images/<%=product.images[1]%>" alt="" class="imgs">
        <img src="./images/<%=product.images[2]%>" alt="" class="imgs">
        <img src="./images/<%=product.images[3]%>" alt="" class="imgs">
        <img src="./images/<%=product.images[4]%>" alt="" class="imgs">
      </div>

    </div>
  </div>

  <hr>
  <div class="details container2">
    <button class="btn btn-success request-btn">Request Call</button>
    <h1>
      <%=product.name%>
    </h1>
    <p><i class="fas fa-thumbtack"></i>
      <%=product.locality%>
        <%=product.city%>
          <%=product.state%>, <%=product.postal%>
    </p>


    <div class="detail-box">
      <div class="small-detail">
        <p><i class="fas fa-bed tags"></i>
          <%=product.bedroom%> Bedroom
        </p>
      </div>
      <div class="small-detail">
        <p><i class="fas fa-bath tags"></i>
          <%=product.bathroom%> Bathroom
        </p>
      </div>
      <div class="small-detail">
        <p><i class="fas fa-chart-area tags"></i> Area : <%=product.area%> sq ft.</p>
      </div>
      <div class="small-detail other-details">
        <p><i class="fas fa-vector-square tags"></i> Carpet: <%=product.area%> sq ft.</p>
      </div>
      <div class="small-detail">
        <p><i class="fas fa-tags tags"></i> Price: â‚¹ <%=product.price%> lacs</p>
      </div>
      <div class="small-detail other-details">
        <p><i class="fas fa-car tags"></i>Price Inclusive all taxes:
          <%if(product.taxIncluded){%>
            Yes
            <%}else{%>
              No
              <%}%>
        </p>
      </div>
      <div class="small-detail other-details">
        <p><i class="fas fa-exchange-alt tags"></i> Negociable:
          <%if(product.negotiable){%>
            Yes
            <%}else{%>
              No
              <%}%>
        </p>
      </div>
      <div class="small-detail other-details">
        <p><i class="fas fa-tint tags"></i> Water Supply: Yes</p>
      </div>
      <div class="small-detail other-details">
        <p><i class="fas fa-plug tags"></i> Electric Supply: Yes</p>
      </div>
      <div class="small-detail other-details">
        <p><i class="fas fa-balance-scale tags"></i></i> DA Resistered: Yes</p>
      </div>

    </div>
    <button class="btn btn-warning more-btn">More Details +</button>
    <button class="btn btn-warning less-btn">Less Details -</button>
  </div>


