<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css"
    integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous" />
  <link rel="stylesheet" href="./css/bootstrap.css">
  <link rel="stylesheet" href="./css/step_form.css">
  <link rel="stylesheet" href="./css/gallery.css">
  <link rel="stylesheet" href="/css/nav.css">
 
  <link rel="stylesheet" href="./css/services.css">
   


  <title>Document</title>
</head>

<body>

  <%- include('partials/nav') %>
  <!-- -------------------------------nav-------------------------------------- -->

  <div class="homepage">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <div class="heading">
            <h1>ALL SERVICES</h1>
            <a href="tel:+918769258658"><button class="btn btn-success btn-lg">Book Now  <i class="fab fa-whatsapp whtsp"></i> </button></a>
          </div>
        </div>
       
      </div>
    </div>
  </div>
<!-----------------------------  step form ----------------->
<div class="out-container">
  <div class="container1 step_form">
    <div class="wrapper">
      <ul class="steps">
        <li class="is-active">Step 1</li>
        <li>Step 2</li>
        <li>Step 3</li>
        <li>Submit</li>
      </ul>
      <form class="form-wrapper" action="/services" method="post">
        <fieldset class="section is-active">
  
                <h3>Type of service</h3>
          <div class="row1 cf">
            <div class="four col">
              <input type="radio" value="Plumbing" name="type" id="r1" checked>
              <label for="r1">
                <h4>Plumbing</h4>
              </label>
            </div>
            <div class="four col">
              <input type="radio" value="Electricity" name="type" id="r2"><label for="r2">
                <h4>Electric</h4>
              </label>
            </div>
            <div class="four col">
              <input type="radio" value="Rennovation" name="type" id="r3"><label for="r3">
                <h4>Rennovation</h4>
              </label>
            </div>
          </div>
          <div class="button">Next</div>
        </fieldset>
        <fieldset class="section ">
          <h3>Name of the City</h3>
          <div class="row1 cf">
            <input type="text" name="city" id="password" placeholder="Nearby city" required>
          </div>
          <div class="buttonprev">Prev</div>
          <div class="button">Next</div>
        </fieldset>
        <fieldset class="section">
          <h3>Locality</h3>
          <div class="row cf">
            <div class="four col">
              <input type="radio" value="Urben" name="citytype" id="r4" checked>
              <label for="r4">
                <h4>Urben</h4>
              </label>
            </div>
            <div class="four col">
              <input type="radio" value="Sub-urben" name="citytype" id="r5"><label for="r5">
                <h4>Sub-urben</h4>
              </label>
            </div>
            <div class="four col">
              <input type="radio" value="Village" name="citytype" id="r6"><label for="r6">
                <h4>Village</h4>
              </label>
            </div>
          </div>
          <div class="buttonprev">Prev</div>
          <div class="button">Next</div>
        </fieldset>
        <fieldset class="section">
          <h3>Submit Details</h3>
          <input type="text" name="name" id="password" placeholder="name" required>
          <input type="tel" name="phone" id="password" placeholder="Phone Number" required>
          <input type="email" name="email" id="password2" placeholder="email(optional)">
          <div class="buttonprev">Prev</div>
          <button type="submit" class="button1">Submit</button>
        </fieldset>
  
      </form>
    </div>
  </div>
</div>
  <!-- --------------------------homepage--------------------------------------------- -->

  <div class="services container" id="services">
    <div class="row">
      <h1>Our Services</h1>
    </div>
    <div class="row">
      <div class="col-lg-3 col-md-6">
        <div class="service-item item1">
          <div class="inner">
            <h2><b>Plumbing</b></h2>
          </div>
        </div>
      </div>
      <div class="col-lg-3 col-md-6">
        <div class="service-item item2">
          <div class="inner">
            <h2><b>Electric</b></h2>
          </div>
        </div>
      </div>
      <div class="col-lg-3 col-md-6">
        <div class="service-item item3">
          <div class="inner">
            <h2><b> Rennovation</b></h2>
          </div>
        </div>
      </div>
      <div class="col-lg-3 col-md-6">
        <div class="service-item item4">
          <div class="inner">
            <h2><b>Interior</b></h2>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- -------------------------------------sec 3-------------------- -->

  <%- include('partials/middle') %>

  <script src="js/jquery.min.js"></script>
  <script src="./js/nav.js"></script>
  <script src="./js/step_form.js"></script>
</body>

</html>