    <form method="post" action="./">
    <!-- filter -->
    <div class="container-out">
        <div class="row">
            <div class="col-lg-3">
           
                    <div class="container1">
                        <div class="filter_section">
                            <h4 class="filter_h heading">Filters</h4>
                     
                            <div class="filter">
                                <div class="property">
                                    <h5>Property Type</h5>
                                    <div class="row mt-2">
                                        <div class="col-sm-4 ">
                                            <div class="option appartment">
                                                <label for="Appartment" class="box_bkcgrd"></label>
                                                <input type="checkbox" id="Appartment" name="type1" value="appartment" >
                                                <div class="box_cover"></div>
                                                <div class="icon">
                                                    <i class="far fa-building"></i>
                                                </div>
                                                <label for="Apartment">Apartment</label>
                                            </div>
                                        </div>
                                        <div class="col-sm-4 ">
                                            <div class="option appartment">
                                                <label for="plot" class="box_bkcgrd"></label>
                                                <input type="checkbox" id="plot" name="type1" value="plot">
                                                <div class="box_cover"></div>
                                                <div class="icon">
                                                    <i style="width:auto" class="fas fa-home"></i>
                                                </div>
                                                <!-- <label style="width:0px;height:0px;appearance: None;" for="Appartment" >APPARTMENT</label> -->
                                                <label for="plot">Plot/Land</label>
                                            </div>
                                        </div>
                                        <div class="option villa">
                                            <label for="villa" class="box_bkcgrd"></label>
                                            <input type="checkbox" id="villa" name="type1" value="villa" >
                                            <div class="box_cover"></div>
                                            <div class="icon">
                                                <i style="width:auto" class="fas fa-laptop-house"></i>
                                            </div>
                                            <!-- <label style="width:0px;height:0px;appearance: None;" for="Appartment" >APPARTMENT</label> -->
                                            <label for="villa">Villa</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="options">
                                    <h5>Commercial</h5>
                                    <!-- Rounded switch -->
                                    <label class="switch">
                                        <input type="checkbox" name="type2" value="commercial" >
                                        <span class="slider round"></span>
                                    </label>
                                </div>

                                <div class="options">
                                    <h5>Residential&ThinSpace; </h5>
                                    <!-- Rounded switch -->
                                    <label class="switch">
                                        <input type="checkbox" name="type2" value="residential">
                                        <span class="slider round"></span>
                                    </label>
                                </div>
                                <br>
                                <div class="budget">
                                    <div class="cont">
                                        <div>
                                            <h5>Budget</h5>
                                        </div>
                                        <div class="values">
                                            <span id="range1">
                                                30
                                            </span>
                                            <span class="lacs">&ThinSpace;cr</span>
                                            <span class="dash">&dash;</span>
                                            <span id="range2">70</span>
                                            <span class="lacs">&ThinSpace;cr</span>
                                        </div>
                                    </div>
                                    <div class="containers containers1">
                                        <div class="slider-track"></div>
                                        <input type="range" id="slider-1" min="0" max="100" value="30"
                                            name="range1" oninput="slideOne()">
                                        <input type="range" min="0" max="100" value="70" id="slider-2"
                                            name="range2" oninput="slideTwo()">
                                    </div>

                                </div>
                                <div class="property ">
                                    <h5>Bedrooms</h5>
                                    <div class="options">
                                        <div class="box" id="box1">
                                            <label for="opt1" class="box_bkcgrd"></label>
                                            <input type="checkbox" id="opt1" name="bedrooms" value="1">
                                            <div class="box_cover"></div>
                                            <label>1</label>
                                        </div>
                                        <div class="box" id="box2">
                                            <label for="opt2" class="box_bkcgrd"></label>
                                            <input type="checkbox" id="opt2" name="bedrooms" value="2">
                                            <div class="box_cover"></div>
                                            <label>2</label>
                                        </div>
                                        <div class="box" id="box3">
                                            <label for="opt3" class="box_bkcgrd"></label>
                                            <input type="checkbox" id="opt3" name="bedrooms" value="3">
                                            <div class="box_cover"></div>
                                            <label>3</label>
                                        </div>
                                        <div class="box" id="box4">
                                            <label for="opt4" class="box_bkcgrd"></label>
                                            <input type="checkbox" id="opt4" name="bedrooms" value="4">
                                            <div class="box_cover"></div>
                                            <label>4</label>
                                        </div>
                                        <div class="box" id="box5">
                                            <label for="opt5" class="box_bkcgrd"></label>
                                            <input type="checkbox" id="opt5" name="bedrooms" value="5">
                                            <div class="box_cover"></div>
                                            <label>5</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="property">
                                    <h5>Bathrooms</h5>
                                    <div class="options">
                                        <div class="box" id="box6">
                                            <label for="opt6" class="box_bkcgrd"></label>
                                            <input type="checkbox" id="opt6" name="bathrooms" value="1">
                                            <div class="box_cover"></div>
                                            <label for="opt6">1</label>
                                        </div>
                                        <div class="box" id="box7">
                                            <label for="opt7" class="box_bkcgrd"></label>
                                            <input type="checkbox" id="opt7" name="bathrooms" value="2">
                                            <div class="box_cover"></div>
                                            <label for="opt7">2</label>
                                        </div>
                                        <div class="box" id="box8">
                                            <label for="opt8" class="box_bkcgrd"></label>
                                            <input type="checkbox" id="opt8" name="bathrooms" value="3">
                                            <div class="box_cover"></div>
                                            <label for="opt8">3</label>
                                        </div>
                                        <div class="box" id="box9">
                                            <label for="opt9" class="box_bkcgrd"></label>
                                            <input type="checkbox" id="opt9" name="bathrooms" value="4">
                                            <div class="box_cover"></div>
                                            <label for="opt9">4</label>
                                        </div>
                                        <div class="box" id="box10">
                                            <label for="opt10" class="box_bkcgrd"></label>
                                            <input type="checkbox" id="opt10" name="bathrooms" value="5">
                                            <div class="box_cover"></div>
                                            <label for="opt10">5</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="area">
                                    <div class="cont">
                                        <div>
                                            <h5>Area</h5>
                                        </div>
                                        <div class="values">
                                            <span id="range3">300</span>
                                            <span class="sqfeet">&MediumSpace; Sq.ft.-</span>
                                            <span id="range4">700</span>
                                            <span class="sqfeet">&MediumSpace; Sq.ft.</span>
                                        </div>
                                    </div>
                                    <div class="containers">
                                        <div class="slider-track"></div>
                                        <input type="range" id="range1_area" name="range1" min="0" max="3000" value="300" oninput="slider_area1()">
                                        <input type="range" min="0" max="3000" value="700" id="range2_area" name="range2"
                                        oninput="slider_area2()">
                                    </div>
                                </div>
                                <div class="age">
                                    <h5>Property Age</h5>
                                    <div><input type="checkbox"  id="one" class="age-checkbox">
                                        <label for="one">0-2 years</label>
                                    </div>
                                    <div><input type="checkbox"  id="two" class="age-checkbox">
                                        <label for="one">2-5 years</label>
                                    </div>
                                    <div><input type="checkbox"  id="three" class="age-checkbox">
                                        <label for="one">5-15 years</label>
                                    </div>
                                    <div><input type="checkbox"  id="four" class="age-checkbox">
                                        <label for="one">15-40 years</label>
                                    </div>
                                    <div><input type="checkbox"  id="five" class="age-checkbox">
                                        <label for="one">40+ years</label>
                                    </div>
                                </div>
                            </div>

                            <div class="vl"></div>
                            <div class="button_filter">
                                <div class="buttons">
                                    <div><button class="reset" onclick="reset()">
                                            <h3>Reset</h3>
                                        </button></div>
                                    <div><button class="apply" type="submit">
                                            <h3>Apply</h3>
                                        </button></div>
                                </div>
                            </div>
                        </div>

                    </div>
             
            </div>
            <div class="col-lg-9">
                <!--Search Bar-->
                <div class="row">
                    <div class="col2">
                        <div>

                            <div class="searchbar">
                                <div class="part1">
                               
                                   
                                                <select name="country" class="countries form-control" id="countryId">
                                              <option value="">Select Country</option>
                                          </select>
                                        
                                     
                                                <select name="state" class="states form-control" id="stateId">
                                              <option value="">Select State</option>
                                          </select>
                                       
                                                <select name="city" class="cities form-control" id="cityId">
                                              <option value="">Select City</option>
                                          </select>
                                       
                                
                                    <div class="search">
                                        <div class="icon">
                                            <i class="fa fa-search"></i>
                                        </div>
                                        
                                    </div>
                                    <div class="search_button">
                                        <button type="submit" class="btn btn-primary">Search</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                       
                    </div>
                   <div class="row mdet">
                        <div class="result-count">
                            <p>Result:  <%=products.length%> properties found</p>
                            <div class="sort-by">
                                <i class="fas fa-filter"></i> 
                                <select name="" id="">
                                    <option value="">area</option>
                                    <option value="">price</option>
                                </select>
                           </div>
                        </div>
                   </div>
                </div>
        <%if(typeof products!='undefined'){%>

                    <div class="row cards">

        <% for(var i=0;i<products.length;i++){%>
                            <div class="col-lg-4">
                                <div class="card">
                                    <img src="./images/<%=products[i].images[0]%>" class="main-image" alt="">
                                    <div class="details">
                                        <h4><%=products[i].name%></h4>
                                        <p><%=products[i].locality%> <%=products[i].city%> <%=products[i].state%>, <%=products[i].postal%></p>
                                        <div class="icon-details">
                                            <div class="bedroom">
                                                <i class="fas fa-bed tags"></i>
                                                <p><%=products[i].bedroom%> Bedroom</p>
                                            </div>
                                            <div class="bathroom">
                                                <i class="fas fa-bath tags"></i>
                                                <p><%=products[i].bathroom%> Bathroom</p>
                                            </div>
                                            <div class="area">
                                                <i class="fas fa-chart-area tags"></i>
                                                <p><%=products[i].area%> sq.ft</p>
                                            </div>
                                        </div>
                                        <div class='card-bottom'>
                                            <div class="price">₹ <%=products[i].price%> lacs</div>
                                            <a href="./page3/<%=products[i]._id%>"><button class="btn btn-sm btn-success card-btn" > More Details</button></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
        <%}%>

                    </div>
           
                    <%}else{%>
                        <div class="container" style="text-align: center;color: darkcyan;margin-top: 20rem;">
                            <h1>NO RESULTS FOUND</h1>
                        </div>
                     <% } %>

            </div>
        </div>
        <div class="row">
            <div class="pointers">
                <button class="btn btn-lg btn-danger"> <<  previos</button>
                <button class="btn btn-lg btn-success next-btn" >Next  >></button>
            </div>
        </div>
    </div>
</form>
